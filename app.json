{
  "expo": {
    "scheme": "aichat501",
    "name": "AiChat501",
    "slug": "AiChat501",
    "version": "1.0.6",
    "orientation": "portrait",
    "icon": "./assets/icons/iconFinal.png",
    "userInterfaceStyle": "light",
    "plugins": [

      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Allow MIRACLE AI to access your microphone.",
          "speechRecognitionPermission": "Allow MIRACLE AI to securely transcribe your Audio input."
        }
      ],
      [
        "expo-document-picker",
        {
          "iCloudContainerEnvironment": "Production"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Give MIRACLE AI access to your Gallery.",
          "cameraPermission": "Allow MIRACLE AI to access your Camera."
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": "Allow MOTHER to access your microphone."
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps._some_id_here_"
        }
      ],
      "expo-secure-store"
    ],
    "splash": {
      "image": "./assets/icons/iconFinal.png",
      "resizeMode": "cover"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "usesIcloudStorage": true,
      "supportsTablet": true,
      "bundleIdentifier": "com.wired87.AiChat501",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "versionCode": 7,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icons/iconFinal.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.wired87.AiChat501",
      "splash": {
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/icons/iconFinal.png"
    },
    "extra": {
      "eas": {
        "projectId": "5f9f8766-f604-4b83-b922-ace89299d73e"
      }
    }
  },
  "react-native-google-mobile-ads": {
    "android_app_id": "ca-app-pub-2225753085204049~1255976039"
  }
}
